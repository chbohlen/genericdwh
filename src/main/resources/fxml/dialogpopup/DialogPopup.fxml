<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="85.0" prefWidth="285.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="lMessage1" alignment="CENTER" layoutX="7.5" layoutY="10.0" prefHeight="20.0" prefWidth="270.0" wrapText="true" />
      <Label fx:id="lMessage2" alignment="CENTER" layoutX="7.5" layoutY="30.0" prefHeight="20.0" prefWidth="270.0" wrapText="true" />
      <HBox fx:id="buttonContainer" alignment="CENTER" layoutX="7.5" layoutY="47.0" prefHeight="46.0" prefWidth="271.0">
         <children>
            <Button fx:id="btnYes" defaultButton="true" mnemonicParsing="false" onAction="#buttonYesOnClickHandler" prefWidth="60.0" text="Yes" />
            <Button fx:id="btnNo" mnemonicParsing="false" onAction="#buttonNoOnClickHandler" prefWidth="60.0" text="No">
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="btnCancel" cancelButton="true" mnemonicParsing="false" onAction="#buttonCancelOnClickHandler" prefWidth="60.0" text="Cancel">
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
